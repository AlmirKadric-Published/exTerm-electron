{
  "name": "exTerm",
  "description": "A modern cross platform terminal multiplexer, heavily inspired by iTerm, built using electron and web technologies",
  "version": "0.1.0",
  "homepage": "https://github.com/AlmirKadric-org/exTerm-electron",
  "license": "MIT",
  "author": {
    "name": "Almir Kadric",
    "email": "github@almirkadric.com",
    "url": "https://github.com/AlmirKadric"
  },
  "engines": {
    "node": "6.5.0"
  },
  "scripts": {
    "deps": "npm install . && install-app-deps",
    "dev": "electron ./app --dev",
    "test": "run-s test:lint",
    "test:lint": "eslint .",
    "icons:all": "run-s icons:mac icons:win",
    "icons:mac": "iconutil -c icns --output ./build/icon.icns ./visual_identity/icon/exTerm.iconset",
    "icons:win": "icotool -c -o ./build/icon.ico ./visual_identity/icon/exTerm.iconset/icon_16x16.png ./visual_identity/icon/exTerm.iconset/icon_32x32.png ./visual_identity/icon/exTerm.iconset/icon_64x64.png ./visual_identity/icon/exTerm.iconset/icon_128x128.png ./visual_identity/icon/exTerm.iconset/icon_256x256.png",
    "build": "webpack -p && build",
    "build:all": "webpack -p && build -mwl"
  },
  "dependencies": {
    "npm-run-all": "4.0.1",
    "rimraf": "2.5.4"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "electron-builder": "12.3.1",
    "electron-prebuilt": "1.4.13",
    "eslint": "3.2.2",
    "eslint-plugin-react": "6.0.0",
    "file-loader": "0.9.0",
    "font-awesome": "4.7.0",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "lodash": "4.17.4",
    "mobx": "2.4.2",
    "mobx-react": "3.5.4",
    "node-uuid": "1.4.7",
    "pre-commit": "1.2.2",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "xterm": "2.3.1"
  }
}
